<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--namespace接口的全路径，用于关联dao层接口-->
<mapper namespace="com.jxd.studentManage.mapper.IPerformenceMapper">
    <insert id="addPerformence">
        insert into performence(markid,studentid,age) values
        <foreach collection="marks" item="mark" separator=",">
            (#{mark.markId},#{studentid},#{age})
        </foreach>
    </insert>

</mapper>